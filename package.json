{
  "name": "@hyphen/react-sdk",
  "version": "1.0.1",
  "description": "Hyphen SDK for React",
  "type": "module",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "biome check --write --error-on-warnings",
    "test": "pnpm lint && vitest run --coverage",
    "test:ci": "biome check --error-on-warnings && vitest run --coverage",
    "build": "rimraf ./dist && tsup src/index.ts --format esm --dts",
    "clean": "rimraf ./dist pnpm-lock.yaml node_modules coverage",
    "prepare": "rimraf ./dist && pnpm build"
  },
  "keywords": [
    "hyphen",
    "sdk",
    "browser",
    "react",
    "javascript",
    "typescript"
  ],
  "author": "Team Hyphen <hello@hyphen.ai>",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@faker-js/faker": "10.1.0",
    "@swc/core": "1.15.3",
    "@testing-library/react": "16.3.0",
    "@types/node": "24.10.1",
    "@types/react": "19.2.7",
    "@vitest/coverage-v8": "4.0.15",
    "dotenv": "17.2.3",
    "happy-dom": "20.0.11",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "rimraf": "6.1.2",
    "tsd": "0.33.0",
    "tsup": "8.5.1",
    "typescript": "5.9.3",
    "vitest": "4.0.15"
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "dependencies": {
    "@hyphen/browser-sdk": "1.0.3",
    "hookified": "1.13.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  }
}
